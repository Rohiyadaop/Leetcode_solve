class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        bool flag  = false ; 
        for(int i = 0;i<=bits.size()-1 ; i++){
            if(bits[i]==1){
                i++; 
                flag = false ; 
            }else{
                flag  = true ; 
            }
        }
        return flag; 
    }
};

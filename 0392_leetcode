class Solution {
public:
    bool isSubsequence(string s, string t) {
       int  count  = 0 ;
       int p = 0; 
        for(int i = 0 ; i<t.size();i++){
             if(s[p]==t[i]){
                count++;
                p++;
             } 
            
        }
        if(count == s.size()){
        return true ;
        }
        else{
            return false ;
        }
    }
};

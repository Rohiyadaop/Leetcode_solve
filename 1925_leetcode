class Solution {
public:
    int countTriples(int n) {
        unordered_set<int> sq;
        for(int i = 1; i <= n; i++) {
            sq.insert(i*i);
        }

        int count = 0;
        for(int a = 1; a <= n; a++) {
            for(int b = 1; b <= n; b++) {
                if(sq.count(a*a + b*b)) {
                    count++;
                }
            }
        }
        return count;
    }
};
